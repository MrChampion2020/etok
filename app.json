{
  "expo": {
    "name": "etok",
    "slug": "etok",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/blueprofile.png",
    "userInterfaceStyle": "light",
     
    "experiments": {
      "baseUrl": "/etok"
    },
    "splash": {
      "image": "./assets/profile.png",
      "resizeMode": "contain",
      "backgroundColor": "white"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/profile.png",
        "backgroundColor": "white"
      }
    },
    "scheme": "etok",
    "web": {
      "favicon": "./assets/profile.png",
      "output": "server",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}